{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "DSharpPlus.BaseDiscordClient": "Warning",
      "Microsoft.Hosting.Lifetime": "Warning",
      "Quartz": "Warning",
      "Microsoft": "Error",
      "System.Net.Http.HttpClient": "Error"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "${ConnectionStrings:DefaultConnection}",
    "DiscordClientSecret": "${ConnectionStrings:DiscordClientSecret}"
  },
  "Steam": {
    "ApiKey": "${Steam:ApiKey}"
  },
  "Riot": {
    "ApiKey": "${Riot:ApiKey}"
  },
  "Lavalink": {
    "Host": "${Lavalink:Host}",
    "Port": "${Lavalink:Port}",
    "Password": "${Lavalink:Password}"
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:6000"
      }
    }
  }
}